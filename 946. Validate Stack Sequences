class Solution {
public:
    bool validateStackSequences(vector<int>& pushed, vector<int>& popped) {
        int n=pushed.size();
        //when stack is not empty so all operation are not performed well return false else return true 
        stack<int>st;
        int i=0,j=0;
        while(i<n&&j<n)
            {    //first stack is empty 
                st.push(pushed[i]);
                while(!st.empty()&& popped[j]==st.top()){  //jab tak top ke Barabar hai pop kro 
                    st.pop();
                    j++;
                }
                i++;
            }
        return st.empty()?true:false;
    }
};
