class Solution {
public:
    vector<string> buildArray(vector<int>& target, int n) {
        int stream =1;
        vector<string>ans;
        int i=0;
        while(i<target. size()&& stream<=n)
            {
                //stream se nikala hai to push karna padega 
                ans.push_back("Push");
                if(target[i]==stream)
                {
                    i++; 
                }// Move for next element 
                else {  //agar barabar nahi hai
                    ans.push_back("Pop");
                }
                stream++;
            }
        return ans;
    }
};
