class Solution {
public:
    int solve(int &a,int &b,string &val){
        if(val=="+")return a+b;
        if(val=="-")return a-b;
        if(val=="*")return a*b;
        if(val=="/")return a/b;
        return -1;
    }
    int evalRPN(vector<string>& tokens) {
        int sum=0;
        int n=tokens.size();
        stack<int>st;
        for(auto val:tokens){
           if(val=="+" ||val=="-"||val=="*"||val=="/"){
            int b=st.top();
            st.pop();
            int a=st.top();
            st.pop();
            sum=solve(a,b,val);
            st.push(sum);
           }
           else st.push(stoi(val)); 
        }
        return st.top();
    }
};
