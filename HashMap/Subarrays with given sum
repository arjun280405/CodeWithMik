class Solution {
  public:
    int subArraySum(vector<int>& arr, int k) {
       unordered_map<int,int>mp;
       int sum=0,count=0;
       for(int i=0;i<arr.size();i++)
       {
           sum+=arr[i];
           if(sum==k)count++;
           //kisi ek element ko hatane se sum equal to k mil rha hai so add freq of that ele in count
           if(mp.find(sum-k)!=mp.end()){
               count+=mp[sum-k];
           }
           mp[sum]++;
       }
       return count;
    }
};
